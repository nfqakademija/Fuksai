<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Article" table="article">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="url" type="string" column="url" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="explanation" type="string" column="explanation" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="string" column="date" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
