<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Planet" table="planet">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="text" column="description" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="position" type="integer" column="position" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="discovery" type="string" column="discovery" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="namedAs" type="string" column="named_as" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="diameter" type="string" column="diameter" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="orbit" type="string" column="orbit" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="day" type="string" column="day" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
